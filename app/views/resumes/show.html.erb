<head>
    <title>responsive web</title>
    <link rel="stylesheet" type="text/css" href="style.css">
    <style>
       
    body{
   padding-left: 90px;
    padding-right: 90px;
    margin:0px;
    background: black;
    }

    .nav{
    width:100%;
    height:80px;
    background:gray;
    }

    ul{
    list-style:none;
    padding:0px;
    margin: 0px;
    position:absolute;
    }

    ul li{
    float:left;
    margin-top: 20px;
    }
    ul li a{
    width: 150px;
    display:block;
    color: white;
    text-decoration: none;
    padding:10px;
    border-radius: 10px;
    text-align: center;
    font-size: 20px;
    font-weight: bolder;
    font-family: Century Gothic;

    }
    a:hover{
    }

    ul li ul{
    background: gray;
    display: none;
    }
    ul li ul li{
    float: none;
    }
    ul li:hover ul{
    display: block;
    }
    p{
        margin-left: 50px;
    }

    }
</style>

</head>
<body>
    <div class="nav">
        <ul>
            <li><a>Home</a></li>
            <li><a>New movies</a></li>
            <li><a>Contact</a></li>
            <li><a>Feedback</a></li>
           
        </ul>
    </div>
</body>
<div class="p">
<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @resume.title %>
</p>

<p>
  <strong class="text text-warning">Categories:</strong>
  <%= @resume.categories %>
</p>

<p>
  <strong>Price:</strong>
  <%= @resume.price %>
</p>

<p>
  <strong>Year:</strong>
  <%= @resume.year %>
</p>

<p>
  <strong>Attachment:</strong>
  <img src="<%= @resume.attachment %>">
</p>

<%= link_to 'Edit', edit_resume_path(@resume) %> |
<%= link_to 'Back', resumes_path %>
</div>
<footer style="background-color: gray;margin-top: 100px;">
    <div class="nav">
        <ul>
            <li><a>Home</a></li>
            <li><a>New movies</a></li>
            <li><a>Contact</a></li>
            <li><a>About</a></li>
        </ul>
    </div>
</footer> 